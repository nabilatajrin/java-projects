    }
    
    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
    
        if ((x%2) == 0){    
            System.out.println("true");    
        } else {
            System.out.println("false"); 
        }  
    } 
